<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>com.servlet.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Create</servlet-name>
        <servlet-class>com.servlet.Create</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addBio</servlet-name>
        <servlet-class>RestfulDispatch.addBio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RestfulBio</servlet-name>
        <servlet-class>RestfulView.RestfulBio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>findapoll</servlet-name>
        <servlet-class>com.servlet.Findpoll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RestfulPoll</servlet-name>
        <servlet-class>RestfulView.RestfulPoll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PollSql</servlet-name>
        <servlet-class>com.servlet.PollSql</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addPoll</servlet-name>
        <servlet-class>RestfulDispatch.addPoll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vote2</servlet-name>
        <servlet-class>com.servlet.vote2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/formdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Create</servlet-name>
        <url-pattern>/Create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addBio</servlet-name>
        <url-pattern>/addBio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RestfulBio</servlet-name>
        <url-pattern>/RestfulBio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findapoll</servlet-name>
        <url-pattern>/findpoll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RestfulPoll</servlet-name>
        <url-pattern>/RestfulPoll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PollSql</servlet-name>
        <url-pattern>/PollSql</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addPoll</servlet-name>
        <url-pattern>/addPoll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vote2</servlet-name>
        <url-pattern>/vote2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
